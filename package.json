{
  "name": "florent-legall-dot-com",
  "private": true,
  "description": "My personal blog",
  "version": "0.1.0",
  "author": "Florent Le Gall <florent.legall@gmail.com>",
  "dependencies": {
    "@babel/core": "7.10.2",
    "fp-ts": "2.6.2",
    "gatsby": "2.22.19",
    "gatsby-image": "2.4.6",
    "gatsby-plugin-catch-links": "2.3.4",
    "gatsby-plugin-feed": "2.5.4",
    "gatsby-plugin-google-analytics": "2.3.3",
    "gatsby-plugin-manifest": "2.4.10",
    "gatsby-plugin-netlify": "2.3.4",
    "gatsby-plugin-offline": "3.2.8",
    "gatsby-plugin-react-helmet": "3.3.3",
    "gatsby-plugin-sharp": "2.6.10",
    "gatsby-plugin-typography": "2.5.3",
    "gatsby-plugin-webpack-bundle-analyzer": "1.0.5",
    "gatsby-remark-autolink-headers": "2.3.4",
    "gatsby-remark-copy-linked-files": "2.3.4",
    "gatsby-remark-embed-snippet": "4.3.3",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "3.3.9",
    "gatsby-remark-prismjs": "3.5.3",
    "gatsby-remark-responsive-iframe": "2.4.4",
    "gatsby-remark-smartypants": "2.3.3",
    "gatsby-source-filesystem": "2.3.10",
    "gatsby-transformer-remark": "2.8.14",
    "gatsby-transformer-sharp": "2.5.4",
    "gravatar": "1.8.0",
    "io-ts": "2.2.4",
    "pluralize": "8.0.0",
    "prismjs": "1.20.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-gravatar": "2.6.3",
    "react-helmet": "6.0.0",
    "react-typography": "0.16.19",
    "recoil": "0.0.8",
    "timeago.js": "4.0.2",
    "typography": "0.16.19",
    "typography-theme-ocean-beach": "0.16.19"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf public .cache",
    "prebuild": "yarn clean",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "prestart": "yarn clean",
    "start": "yarn run develop",
    "prestart:prod": "yarn clean",
    "start:prod": "yarn build && yarn serve",
    "serve": "gatsby serve --port 5020",
    "eslint": "eslint . --cache",
    "tsc": "tsc",
    "test": "jest --verbose",
    "format": "prettier --write \"**/*.js\" \"**/*.json\" \"**/*.css\" \"**/*.md\"",
    "cy:install": "cypress install",
    "cy:open": "cypress open",
    "cy:run": "CYPRESS_BASE_URL=http://localhost:5020 cypress run",
    "test:e2e": "start-server-and-test serve http://localhost:5020 cy:run",
    "test:circle": "npm-run-all --parallel test tsc eslint build",
    "build:full": "npm-run-all --parallel test tsc eslint build cy:install && yarn test:e2e"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "devDependencies": {
    "@babel/preset-env": "7.10.2",
    "@babel/preset-typescript": "7.10.1",
    "@gatsby-contrib/gatsby-plugin-elasticlunr-search": "2.3.0",
    "@testing-library/cypress": "6.0.0",
    "@types/gravatar": "1.8.1",
    "@types/jest": "25.2.3",
    "@types/jquery": "3.3.38",
    "@types/pluralize": "0.0.29",
    "@types/react-dom": "16.9.8",
    "@types/react-gravatar": "2.6.8",
    "@types/react-helmet": "6.0.0",
    "@types/recoil": "0.0.0",
    "@types/typography": "0.16.3",
    "babel-jest": "26.0.1",
    "cypress": "4.7.0",
    "elasticlunr": "0.9.5",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-cypress": "2.11.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-standard": "4.0.1",
    "gatsby-plugin-typescript": "2.4.4",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "lint-staged": "10.2.8",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "remark-parse": "8.0.2",
    "start-server-and-test": "1.11.0",
    "typescript": "3.9.3",
    "unified": "9.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn build:full",
      "post-checkout": "yarn",
      "post-merge": "yarn"
    }
  },
  "lint-staged": {
    "*.{json,css,md,ts,tsx}": [
      "prettier --write"
    ],
    "*.js": [
      "eslint --cache",
      "prettier --write"
    ]
  },
  "resolutions": {
    "cypress": "4.7.0"
  }
}
