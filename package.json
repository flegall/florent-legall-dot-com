{
  "name": "florent-legall-dot-com",
  "private": true,
  "description": "My personal blog",
  "version": "0.1.0",
  "author": "Florent Le Gall <florent.legall@gmail.com>",
  "dependencies": {
    "@babel/core": "7.7.7",
    "constate": "1.3.2",
    "fp-ts": "2.3.1",
    "gatsby": "2.18.18",
    "gatsby-image": "2.2.37",
    "gatsby-plugin-catch-links": "2.1.21",
    "gatsby-plugin-feed": "2.3.25",
    "gatsby-plugin-google-analytics": "2.1.31",
    "gatsby-plugin-manifest": "2.2.34",
    "gatsby-plugin-netlify": "2.1.30",
    "gatsby-plugin-offline": "3.0.30",
    "gatsby-plugin-react-helmet": "3.1.18",
    "gatsby-plugin-sharp": "2.3.10",
    "gatsby-plugin-typography": "2.3.20",
    "gatsby-plugin-webpack-bundle-analyzer": "1.0.5",
    "gatsby-remark-autolink-headers": "2.1.22",
    "gatsby-remark-copy-linked-files": "2.1.33",
    "gatsby-remark-embed-snippet": "4.1.18",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "3.1.39",
    "gatsby-remark-prismjs": "3.3.29",
    "gatsby-remark-responsive-iframe": "2.2.30",
    "gatsby-remark-smartypants": "2.1.19",
    "gatsby-source-filesystem": "2.1.43",
    "gatsby-transformer-remark": "2.6.45",
    "gatsby-transformer-sharp": "2.3.9",
    "gravatar": "1.8.0",
    "io-ts": "2.0.3",
    "pluralize": "8.0.0",
    "prismjs": "1.18.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-gravatar": "2.6.3",
    "react-helmet": "5.2.1",
    "react-typography": "0.16.19",
    "timeago.js": "4.0.2",
    "typography": "0.16.19",
    "typography-theme-ocean-beach": "0.16.19"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf public .cache",
    "prebuild": "yarn clean",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "prestart": "yarn clean",
    "start": "yarn run develop",
    "prestart:prod": "yarn clean",
    "start:prod": "yarn build && yarn serve",
    "serve": "gatsby serve --port 5020",
    "eslint": "eslint . --cache",
    "tsc": "tsc",
    "test": "jest --verbose",
    "format": "prettier --write \"**/*.js\" \"**/*.json\" \"**/*.css\" \"**/*.md\"",
    "cy:install": "cypress install",
    "cy:open": "cypress open",
    "cy:run": "CYPRESS_BASE_URL=http://localhost:5020 cypress run",
    "test:e2e": "start-server-and-test serve http://localhost:5020 cy:run",
    "test:circle": "npm-run-all --parallel test tsc eslint build",
    "build:full": "npm-run-all --parallel test tsc eslint build cy:install && yarn test:e2e"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "devDependencies": {
    "@babel/preset-env": "7.7.7",
    "@babel/preset-typescript": "7.7.7",
    "@gatsby-contrib/gatsby-plugin-elasticlunr-search": "2.3.0",
    "@testing-library/cypress": "5.0.2",
    "@types/gravatar": "1.8.0",
    "@types/jest": "24.0.25",
    "@types/jquery": "3.3.31",
    "@types/pluralize": "0.0.29",
    "@types/react-gravatar": "2.6.7",
    "@types/react-helmet": "5.0.15",
    "@types/typography": "0.16.3",
    "babel-jest": "24.9.0",
    "cypress": "3.8.1",
    "elasticlunr": "0.9.5",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-cypress": "2.8.1",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-standard": "4.0.1",
    "gatsby-plugin-typescript": "2.1.23",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "lint-staged": "9.5.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "remark-parse": "7.0.2",
    "start-server-and-test": "1.10.6",
    "typescript": "3.7.4",
    "unified": "8.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn build:full",
      "post-checkout": "yarn",
      "post-merge": "yarn"
    }
  },
  "lint-staged": {
    "*.{json,css,md,ts,tsx}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --cache",
      "prettier --write",
      "git add"
    ]
  }
}
