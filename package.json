    "gatsby-plugin-offline": "4.8.0",
    "gatsby-plugin-react-helmet": "4.8.0",
    "gatsby-plugin-sharp": "3.8.0",
    "gatsby-plugin-typography": "3.8.0",
    "gatsby-plugin-webpack-bundle-analyzer": "1.0.5",
    "gatsby-remark-autolink-headers": "4.5.0",
    "gatsby-remark-copy-linked-files": "4.5.0",
    "gatsby-remark-embed-snippet": "6.5.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "5.5.0",
    "gatsby-remark-prismjs": "5.5.0",
    "gatsby-remark-responsive-iframe": "4.5.0",
    "gatsby-remark-smartypants": "4.5.0",
    "gatsby-source-filesystem": "3.8.0",
    "gatsby-transformer-remark": "4.5.0",
    "gatsby-transformer-sharp": "3.8.0",
    "gravatar": "1.8.1",
    "io-ts": "2.2.16",
    "pluralize": "8.0.0",
    "prismjs": "1.24.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-gravatar": "2.6.3",
    "react-helmet": "6.1.0",
    "react-typography": "0.16.20",
    "recoil": "0.3.1",
    "timeago.js": "4.0.2",
    "typography": "0.16.21",
    "typography-theme-ocean-beach": "0.16.19"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf public .cache",
    "build": "yarn clean && gatsby build",
    "develop": "gatsby develop",
    "start": "yarn clean && yarn run develop",
    "start:prod": "yarn clean && yarn build && yarn serve",
    "serve": "gatsby serve --port 5020",
    "eslint": "eslint . --cache",
    "tsc": "tsc",
    "test": "jest --verbose",
    "format": "prettier --write \"**/*.js\" \"**/*.json\" \"**/*.css\" \"**/*.md\"",
    "cy:install": "cypress install",
    "cy:open": "cypress open",
    "cy:run": "CYPRESS_BASE_URL=http://localhost:5020 cypress run",
    "test:e2e": "start-server-and-test serve http://localhost:5020 cy:run",
    "build:full": "npm-run-all --parallel test tsc eslint build cy:install && yarn test:e2e",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "devDependencies": {
    "@babel/preset-env": "7.14.7",
    "@babel/preset-typescript": "7.14.5",
    "@gatsby-contrib/gatsby-plugin-elasticlunr-search": "3.0.2",
    "@testing-library/cypress": "7.0.6",
    "@types/gravatar": "1.8.2",
    "@types/jest": "26.0.23",
    "@types/jquery": "3.5.5",
    "@types/pluralize": "0.0.29",
    "@types/react-dom": "17.0.8",
    "@types/react-gravatar": "2.6.8",
    "@types/react-helmet": "6.1.1",
    "@types/recoil": "0.0.1",
    "@types/typography": "0.16.3",
    "babel-jest": "26.6.3",
    "cypress": "7.6.0",
    "elasticlunr": "0.9.5",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-cypress": "2.11.3",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-standard": "5.0.0",
    "gatsby-plugin-typescript": "3.8.0",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "lint-staged": "11.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.2",
    "remark-parse": "9.0.0",
    "start-server-and-test": "1.12.5",
    "typescript": "4.2.4",
    "unified": "9.2.1"
  },
  "lint-staged": {
    "*.{json,css,md,ts,tsx}": [
      "prettier --write"
    ],
    "*.js": [
      "eslint --cache",
      "prettier --write"
    ]
  }
}
