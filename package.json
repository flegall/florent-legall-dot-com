{
  "name": "florent-legall-dot-com",
  "private": true,
  "description": "My personal blog",
  "version": "0.1.0",
  "author": "Florent Le Gall <florent.legall@gmail.com>",
  "dependencies": {
    "@babel/core": "7.14.6",
    "@reach/router": "1.3.4",
    "fp-ts": "2.10.5",
    "gatsby": "3.8.1",
    "gatsby-image": "3.8.0",
    "gatsby-plugin-catch-links": "3.8.0",
    "gatsby-plugin-feed": "3.8.0",
    "gatsby-plugin-manifest": "3.8.0",
