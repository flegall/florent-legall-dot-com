{
  "name": "florent-legall-dot-com",
  "private": true,
  "description": "My personal blog",
  "version": "0.1.0",
  "author": "Florent Le Gall <florent.legall@gmail.com>",
  "dependencies": {
    "@babel/core": "7.16.5",
    "@reach/router": "1.3.4",
    "fp-ts": "2.11.5",
    "gatsby": "4.4.0",
    "gatsby-image": "3.11.0",
    "gatsby-plugin-catch-links": "4.4.0",
    "gatsby-plugin-feed": "4.4.0",
    "gatsby-plugin-manifest": "4.4.0",
