    "gatsby-plugin-offline": "5.4.0",
    "gatsby-plugin-react-helmet": "5.4.0",
    "gatsby-plugin-sharp": "4.4.0",
    "gatsby-plugin-typography": "4.4.0",
    "gatsby-plugin-webpack-bundle-analyzer": "1.0.5",
    "gatsby-remark-autolink-headers": "5.4.0",
    "gatsby-remark-copy-linked-files": "5.4.0",
    "gatsby-remark-embed-snippet": "7.4.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "6.4.0",
    "gatsby-remark-prismjs": "6.4.0",
    "gatsby-remark-responsive-iframe": "5.4.0",
    "gatsby-remark-smartypants": "5.4.0",
    "gatsby-source-filesystem": "4.4.0",
    "gatsby-transformer-remark": "5.4.0",
    "gatsby-transformer-sharp": "4.4.0",
    "gravatar": "1.8.2",
    "io-ts": "2.2.16",
    "pluralize": "8.0.0",
    "prismjs": "1.25.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-gravatar": "2.6.3",
    "react-helmet": "6.1.0",
    "react-typography": "0.16.20",
    "recoil": "0.5.2",
    "timeago.js": "4.0.2",
    "typography": "0.16.21",
    "typography-theme-ocean-beach": "0.16.19"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf public .cache",
    "build": "yarn clean && gatsby build",
    "develop": "gatsby develop",
    "start": "yarn clean && yarn run develop",
    "start:prod": "yarn clean && yarn build && yarn serve",
    "serve": "gatsby serve --port 5020",
    "eslint": "eslint . --cache",
    "tsc": "tsc",
    "test": "jest --verbose",
    "format": "prettier --write \"**/*.js\" \"**/*.json\" \"**/*.css\" \"**/*.md\"",
    "cy:install": "cypress install",
    "cy:open": "cypress open",
    "cy:run": "CYPRESS_BASE_URL=http://localhost:5020 cypress run",
    "test:e2e": "start-server-and-test serve http://localhost:5020 cy:run",
    "build:full": "npm-run-all --parallel test tsc eslint build cy:install && yarn test:e2e",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "devDependencies": {
    "@babel/preset-env": "7.16.5",
    "@babel/preset-typescript": "7.16.5",
    "@gatsby-contrib/gatsby-plugin-elasticlunr-search": "3.0.2",
    "@testing-library/cypress": "8.0.2",
    "@types/gravatar": "1.8.3",
    "@types/jest": "27.0.3",
    "@types/jquery": "3.5.10",
    "@types/pluralize": "0.0.29",
    "@types/react-dom": "17.0.11",
    "@types/react-gravatar": "2.6.10",
    "@types/react-helmet": "6.1.4",
    "@types/recoil": "0.0.1",
    "@types/typography": "0.16.4",
    "babel-jest": "26.6.3",
    "cypress": "9.1.1",
    "elasticlunr": "0.9.5",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-standard": "5.0.0",
    "gatsby-plugin-typescript": "4.4.0",
    "husky": "7.0.4",
    "jest": "26.6.3",
    "lint-staged": "12.1.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "remark-parse": "9.0.0",
    "start-server-and-test": "1.14.0",
    "typescript": "4.2.4",
    "unified": "9.2.2"
  },
  "lint-staged": {
    "*.{json,css,md,ts,tsx}": [
      "prettier --write"
    ],
    "*.js": [
      "eslint --cache",
      "prettier --write"
    ]
  }
}
