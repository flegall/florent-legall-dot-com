{
  "name": "florent-legall-dot-com",
  "private": true,
  "description": "My personal blog",
  "version": "0.1.0",
  "author": "Florent Le Gall <florent.legall@gmail.com>",
  "dependencies": {
    "@babel/core": "7.8.4",
    "constate": "1.3.2",
    "fp-ts": "2.4.3",
    "gatsby": "2.19.10",
    "gatsby-image": "2.2.39",
    "gatsby-plugin-catch-links": "2.1.24",
    "gatsby-plugin-feed": "2.3.26",
    "gatsby-plugin-google-analytics": "2.1.34",
    "gatsby-plugin-manifest": "2.2.40",
    "gatsby-plugin-netlify": "2.1.31",
    "gatsby-plugin-offline": "3.0.33",
    "gatsby-plugin-react-helmet": "3.1.21",
    "gatsby-plugin-sharp": "2.4.4",
    "gatsby-plugin-typography": "2.3.21",
    "gatsby-plugin-webpack-bundle-analyzer": "1.0.5",
    "gatsby-remark-autolink-headers": "2.1.23",
    "gatsby-remark-copy-linked-files": "2.1.36",
    "gatsby-remark-embed-snippet": "4.1.21",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "3.1.43",
    "gatsby-remark-prismjs": "3.3.30",
    "gatsby-remark-responsive-iframe": "2.2.31",
    "gatsby-remark-smartypants": "2.1.20",
    "gatsby-source-filesystem": "2.1.47",
    "gatsby-transformer-remark": "2.6.49",
    "gatsby-transformer-sharp": "2.3.13",
    "gravatar": "1.8.0",
    "io-ts": "2.0.6",
    "pluralize": "8.0.0",
    "prismjs": "1.19.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-gravatar": "2.6.3",
    "react-helmet": "5.2.1",
    "react-typography": "0.16.19",
    "timeago.js": "4.0.2",
    "typography": "0.16.19",
    "typography-theme-ocean-beach": "0.16.19"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf public .cache",
    "prebuild": "yarn clean",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "prestart": "yarn clean",
    "start": "yarn run develop",
    "prestart:prod": "yarn clean",
    "start:prod": "yarn build && yarn serve",
    "serve": "gatsby serve --port 5020",
    "eslint": "eslint . --cache",
    "tsc": "tsc",
    "test": "jest --verbose",
    "format": "prettier --write \"**/*.js\" \"**/*.json\" \"**/*.css\" \"**/*.md\"",
    "cy:install": "cypress install",
    "cy:open": "cypress open",
    "cy:run": "CYPRESS_BASE_URL=http://localhost:5020 cypress run",
    "test:e2e": "start-server-and-test serve http://localhost:5020 cy:run",
    "test:circle": "npm-run-all --parallel test tsc eslint build",
    "build:full": "npm-run-all --parallel test tsc eslint build cy:install && yarn test:e2e"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "devDependencies": {
    "@babel/preset-env": "7.8.4",
    "@babel/preset-typescript": "7.8.3",
    "@gatsby-contrib/gatsby-plugin-elasticlunr-search": "2.3.0",
    "@testing-library/cypress": "5.1.1",
    "@types/gravatar": "1.8.0",
    "@types/jest": "25.1.1",
    "@types/jquery": "3.3.31",
    "@types/pluralize": "0.0.29",
    "@types/react-gravatar": "2.6.7",
    "@types/react-helmet": "5.0.15",
    "@types/typography": "0.16.3",
    "babel-jest": "25.1.0",
    "cypress": "3.8.3",
    "elasticlunr": "0.9.5",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-cypress": "2.8.1",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.18.0",
    "eslint-plugin-standard": "4.0.1",
    "gatsby-plugin-typescript": "2.1.26",
    "husky": "4.2.1",
    "jest": "25.1.0",
    "lint-staged": "9.5.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "remark-parse": "7.0.2",
    "start-server-and-test": "1.10.8",
    "typescript": "3.7.5",
    "unified": "8.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn build:full",
      "post-checkout": "yarn",
      "post-merge": "yarn"
    }
  },
  "lint-staged": {
    "*.{json,css,md,ts,tsx}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --cache",
      "prettier --write",
      "git add"
    ]
  }
}
